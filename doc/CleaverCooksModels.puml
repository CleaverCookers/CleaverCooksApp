class Allergy{
    - id : String
    - title : String
}

class Ingredient{
    - id : String
    - title : String
    - allergies : Allergy[]
}


class Receipe{
    - id : String
    - title : String
    - description : String
    - elements : Element[]
}

class Element{
    - amount : String
    - ingredient : Ingredient
}

Allergy <-- Ingredient
Receipe o-- Element
Element --> Ingredient
@enduml